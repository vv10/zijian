	<div class="container">
		<div id="masonry" class="channel container-fluid">
			<% rs.each do |product| %>
				<% cache product do %>
					<div class="service-block service-block-default p-10 box">
						<%= image_tag product.picture.upload.url(:lg)%>
						<p><%= link_to("[#{product.pcode}] #{product.title}", details_path(product.id), :target=> "_blank") %></p>
					</div>
				<% end %>
			<% end %>
		</div>
	</div>


<%= paginate rs, :theme => 'channel' %>
